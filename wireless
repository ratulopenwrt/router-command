
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/18000000.wifi'
	option band '2g'
	option channel '6'
	option htmode 'HE20'
	option country 'CN'
	option cell_density '0'
	option vendor_vht '1'
	option txpower '22'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc/18000000.wifi+1'
	option band '5g'
	option channel '36'
	option htmode 'HE160'
	option country 'CN'
	option cell_density '0'
	option txpower '27'
	option mu_beamformer '1'

config wifi-iface 'wifinet0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'Ratul'
	option encryption 'psk2+ccmp'
	option key 'B!n@ryS3arch#25$'
	option ieee80211r '1'
	option mobility_domain '123A'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option network 'lan'

config wifi-iface 'wifinet1'
	option device 'radio1'
	option mode 'ap'
	option ssid 'Ratul 5G'
	option encryption 'psk2+ccmp'
	option key 'B!n@ryS3arch#25$'
	option ieee80211r '1'
	option mobility_domain '123A'
	option ft_over_ds '0'
	option ft_psk_generate_local '1'
	option network 'lan'

